public class ReverseStrBuild{
	public static void main(String args[]) {
		String s="Mantesh";
		
		System.out.println(s);
		
		StringBuilder sb = new StringBuilder();
		sb.append(s);
		sb.reverse();
		System.out.println(sb);
	}
}






























































